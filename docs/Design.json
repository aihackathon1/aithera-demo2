{
    "designSystem": {
      "name": "Modern AI Learning Platform",
      "version": "1.0",
      "type": "Educational Technology Platform"
    },
    "colorPalette": {
      "primary": {
        "orange": "#FF4500",
        "orangeLight": "#FF6B35",
        "orangeDark": "#E63900"
      },
      "secondary": {
        "purple": "#8B5CF6",
        "purpleLight": "#A78BFA",
        "purpleDark": "#7C3AED"
      },
      "accent": {
        "yellow": "#FDE047",
        "yellowLight": "#FEF08A",
        "green": "#22C55E",
        "greenLight": "#86EFAC"
      },
      "neutral": {
        "white": "#FFFFFF",
        "gray50": "#F9FAFB",
        "gray100": "#F3F4F6",
        "gray200": "#E5E7EB",
        "gray600": "#4B5563",
        "gray900": "#111827"
      },
      "gradients": {
        "heroBackground": "linear-gradient(135deg, #FDE047 0%, #A78BFA 50%, #F3F4F6 100%)",
        "cardAccents": "linear-gradient(45deg, var(--primary-color) 0%, var(--secondary-color) 100%)",
        "ctaBackground": "linear-gradient(135deg, #FF4500 0%, #FF6B35 100%)"
      }
    },
    "typography": {
      "fontFamily": {
        "primary": "Inter, system-ui, -apple-system, sans-serif",
        "fallback": "Arial, Helvetica, sans-serif"
      },
      "scale": {
        "hero": {
          "fontSize": "clamp(2.5rem, 5vw, 4rem)",
          "fontWeight": "700",
          "lineHeight": "1.1",
          "letterSpacing": "-0.02em"
        },
        "heading1": {
          "fontSize": "clamp(2rem, 4vw, 3rem)",
          "fontWeight": "600",
          "lineHeight": "1.2"
        },
        "heading2": {
          "fontSize": "clamp(1.5rem, 3vw, 2rem)",
          "fontWeight": "600",
          "lineHeight": "1.3"
        },
        "body": {
          "fontSize": "1rem",
          "fontWeight": "400",
          "lineHeight": "1.6"
        },
        "caption": {
          "fontSize": "0.875rem",
          "fontWeight": "500",
          "lineHeight": "1.4"
        }
      }
    },
    "spacing": {
      "base": "1rem",
      "scale": {
        "xs": "0.25rem",
        "sm": "0.5rem",
        "md": "1rem",
        "lg": "1.5rem",
        "xl": "2rem",
        "2xl": "3rem",
        "3xl": "4rem",
        "4xl": "6rem"
      },
      "sections": {
        "padding": "clamp(2rem, 5vw, 4rem)",
        "margin": "clamp(3rem, 8vw, 6rem)"
      }
    },
    "borderRadius": {
      "sm": "0.375rem",
      "md": "0.5rem",
      "lg": "0.75rem",
      "xl": "1rem",
      "2xl": "1.5rem",
      "full": "50%"
    },
    "shadows": {
      "card": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "cardHover": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
      "feature": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)"
    },
    "components": {
      "buttons": {
        "primary": {
          "background": "linear-gradient(135deg, #FF4500 0%, #FF6B35 100%)",
          "color": "#FFFFFF",
          "padding": "0.75rem 2rem",
          "borderRadius": "0.5rem",
          "fontWeight": "600",
          "fontSize": "1rem",
          "border": "none",
          "cursor": "pointer",
          "transition": "all 0.2s ease-in-out",
          "hover": {
            "transform": "translateY(-1px)",
            "boxShadow": "0 10px 15px -3px rgba(255, 69, 0, 0.3)"
          }
        },
        "secondary": {
          "background": "transparent",
          "color": "#4B5563",
          "padding": "0.75rem 2rem",
          "borderRadius": "0.5rem",
          "fontWeight": "600",
          "fontSize": "1rem",
          "border": "2px solid #E5E7EB",
          "cursor": "pointer",
          "transition": "all 0.2s ease-in-out"
        }
      },
      "cards": {
        "feature": {
          "background": "#FFFFFF",
          "borderRadius": "1rem",
          "padding": "2rem",
          "boxShadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)",
          "transition": "all 0.3s ease-in-out",
          "hover": {
            "transform": "translateY(-2px)",
            "boxShadow": "0 20px 25px -5px rgba(0, 0, 0, 0.1)"
          }
        },
        "coloredAccent": {
          "borderRadius": "1rem",
          "padding": "1.5rem",
          "position": "relative",
          "overflow": "hidden",
          "variants": {
            "yellow": {
              "background": "linear-gradient(135deg, #FDE047 0%, #FEF08A 100%)"
            },
            "purple": {
              "background": "linear-gradient(135deg, #A78BFA 0%, #C4B5FD 100%)"
            },
            "orange": {
              "background": "linear-gradient(135deg, #FB923C 0%, #FDBA74 100%)"
            },
            "green": {
              "background": "linear-gradient(135deg, #86EFAC 0%, #BBF7D0 100%)"
            }
          }
        }
      },
      "navigation": {
        "header": {
          "background": "rgba(255, 255, 255, 0.9)",
          "backdropFilter": "blur(10px)",
          "borderBottom": "1px solid #E5E7EB",
          "padding": "1rem 2rem",
          "position": "sticky",
          "top": "0",
          "zIndex": "1000"
        },
        "logo": {
          "fontSize": "1.5rem",
          "fontWeight": "700",
          "color": "#111827"
        },
        "menuItems": {
          "fontSize": "0.875rem",
          "fontWeight": "500",
          "color": "#4B5563",
          "padding": "0.5rem 1rem",
          "borderRadius": "0.375rem",
          "transition": "all 0.2s ease-in-out",
          "hover": {
            "background": "#F3F4F6",
            "color": "#111827"
          }
        }
      }
    },
    "layout": {
      "container": {
        "maxWidth": "1200px",
        "margin": "0 auto",
        "padding": "0 1rem"
      },
      "grid": {
        "hero": {
          "display": "grid",
          "gridTemplateColumns": "1fr",
          "gap": "2rem",
          "alignItems": "center",
          "minHeight": "70vh",
          "responsive": {
            "md": "grid-template-columns: 1fr 1fr"
          }
        },
        "features": {
          "display": "grid",
          "gridTemplateColumns": "repeat(auto-fit, minmax(300px, 1fr))",
          "gap": "2rem",
          "margin": "3rem 0"
        },
        "testimonials": {
          "display": "grid",
          "gridTemplateColumns": "repeat(auto-fit, minmax(250px, 1fr))",
          "gap": "1.5rem",
          "margin": "2rem 0"
        }
      },
      "sections": {
        "hero": {
          "background": "linear-gradient(135deg, #FDE047 0%, #A78BFA 50%, #F3F4F6 100%)",
          "padding": "clamp(2rem, 8vw, 6rem) 0",
          "textAlign": "center"
        },
        "features": {
          "background": "#FFFFFF",
          "padding": "clamp(3rem, 8vw, 6rem) 0"
        },
        "cta": {
          "background": "linear-gradient(135deg, #FB923C 0%, #FDBA74 100%)",
          "padding": "clamp(2rem, 6vw, 4rem) 0",
          "textAlign": "center",
          "borderRadius": "1rem",
          "margin": "3rem 0"
        }
      }
    },
    "animations": {
      "transitions": {
        "default": "all 0.2s ease-in-out",
        "slow": "all 0.3s ease-in-out",
        "bounce": "all 0.2s cubic-bezier(0.68, -0.55, 0.265, 1.55)"
      },
      "hover": {
        "lift": "translateY(-2px)",
        "scale": "scale(1.02)",
        "glow": "0 10px 25px -5px rgba(255, 69, 0, 0.3)"
      },
      "keyframes": {
        "fadeInUp": {
          "from": {
            "opacity": "0",
            "transform": "translateY(20px)"
          },
          "to": {
            "opacity": "1",
            "transform": "translateY(0)"
          }
        },
        "pulse": {
          "0%": { "opacity": "1" },
          "50%": { "opacity": "0.7" },
          "100%": { "opacity": "1" }
        }
      }
    },
    "responsive": {
      "breakpoints": {
        "sm": "640px",
        "md": "768px",
        "lg": "1024px",
        "xl": "1280px"
      },
      "patterns": {
        "mobileFirst": true,
        "fluidTypography": "clamp() functions for scalable text",
        "flexibleLayouts": "CSS Grid with auto-fit and minmax",
        "adaptiveSpacing": "clamp() for consistent spacing across devices"
      }
    },
    "designPrinciples": {
      "visualHierarchy": {
        "emphasis": "Bold gradients and contrasting colors for CTAs",
        "spacing": "Generous whitespace between sections",
        "typography": "Clear size distinctions between headings and body text"
      },
      "accessibility": {
        "contrast": "Minimum 4.5:1 ratio for text",
        "focusStates": "Visible focus indicators on interactive elements",
        "semantics": "Proper heading hierarchy and ARIA labels"
      },
      "modernAesthetic": {
        "gradients": "Subtle gradients for depth and visual interest",
        "roundedCorners": "Consistent border-radius throughout",
        "cardDesign": "Elevated cards with subtle shadows",
        "colorfulAccents": "Vibrant colored sections for engagement"
      }
    },
    "contentStructure": {
      "hero": {
        "elements": ["headline", "subheadline", "primaryCTA", "visualElements"],
        "layout": "Center-aligned with strong visual hierarchy"
      },
      "features": {
        "structure": "Grid of feature cards with icons and descriptions",
        "pattern": "Icon + Headline + Description format"
      },
      "social_proof": {
        "elements": ["client logos", "testimonials", "statistics"],
        "placement": "Throughout the page for credibility"
      },
      "navigation": {
        "style": "Clean horizontal navigation with clear hierarchy",
        "elements": ["logo", "menu items", "user actions"]
      }
    }
  }